            <TableColumn prefWidth="62.0" text="Add Date" />
            <TableColumn prefWidth="75.0" text="Stock Id" />
            <TableColumn prefWidth="79.0" text="Badge Id" />
            <TableColumn prefWidth="95.0" text="Buy Price" />
            <TableColumn prefWidth="90.0" text="Sale Price" />
            <TableColumn prefWidth="83.0" text="ExpDate" />
            <TableColumn prefWidth="89.0" text="Item Id" />
            <TableColumn prefWidth="87.0" text="Qty" />
         </columns>
      </TableView>
      <ImageView fx:id="printbtn" layoutX="835.0" layoutY="504.0" onMouseClicked="#printReportAction">
         <image>
            <Image url="@../image/naviIcon/print_48px.png" />
         </image>
      </ImageView>
      <Label layoutX="371.0" layoutY="36.0" text="Stock View" textFill="#0d47a1">
